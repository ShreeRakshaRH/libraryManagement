package com.capgemini.libraryManagement.beans;

import javax.persistence.Column;
import javax.persistence.Id;

public class User {
	@Id
	@Column(name = "id")
	private Integer id;
	@Column(name = "name")
	private String name;
	@Column(name = "email")
	private String email;
	@Column(name = "password")
	private String password;
	@Column(name = "contact")
	private Long contact;
	@Column
	private String type;
	
}
